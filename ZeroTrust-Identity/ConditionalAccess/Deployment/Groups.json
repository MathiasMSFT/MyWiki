{
    "Personas": [
        {
            "id": "P000",
            "Name": "P-Breakglass",
            "Type": "manual",
            "Rule": "",
            "Description": "Contains 2 Breakglass accounts",
            "IsAssignableToRole": true,
            "RestrictedAU": true,
            "RestrictedAUName": "CAP-Exceptions"
        },
        {
            "id": "P100",
            "Name": "P-Admins",
            "Type": "dynamic",
            "Rule": "(user.userPrincipalName -startswith \"adm-\")",
            "Description": "Contains all administrators based on their userPrincipalName",
            "IsAssignableToRole": false,
            "RestrictedAU": true,
            "RestrictedAUName": "CAP-Exceptions"
        },
        {
            "id": "P200",
            "Name": "P-Internals",
            "Type": "dynamic",
            "Rule": "(user.companyname -eq \"contoso\")",
            "Description": "Contains all employees based on the companyname attribute",
            "IsAssignableToRole": false,
            "RestrictedAU": false
        },
        {
            "id": "P300",
            "Name": "P-Externals",
            "Type": "dynamic",
            "Rule": "(user.companyname -ne \"contoso\" AND user.userType -ne \"guest\")",
            "Description": "Contains all external peoples based on companyname and type of identity",
            "IsAssignableToRole": false,
            "RestrictedAU": false
        },
        {
            "id": "P400",
            "Name": "P-Guests",
            "Type": "dynamic",
            "Rule": "(user.userType -eq \"guest\")",
