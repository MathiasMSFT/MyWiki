            "IsAssignableToRole": false,
            "RestrictedAU": false
        },
        {
            "id": "P500",
            "Name": "P-GenericAccount",
            "Type": "dynamic",
            "Rule": "(user.userPrincipalName -startswith \"g-\")",
            "Description": "Containts all generic accounts based on their userPrincipalName",
            "IsAssignableToRole": false,
            "RestrictedAU": false
        },
        {
            "id": "P900",
