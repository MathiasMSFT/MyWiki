    "Personas": [
        {
            "id": "P000",
            "Name": "P-Breakglass",
            "Type": "manual",
            "Rule": "",
            "Description": "Contains 2 Breakglass accounts",
            "IsAssignableToRole": true,
            "RestrictedAU": true,
            "RestrictedAUName": "Protected-Personas"
        },
        {
            "id": "P100",
            "Name": "P-Admins",
            "Type": "dynamic",
            "Rule": "(user.userPrincipalName -startswith \"adm-\")",
            "Description": "Contains all administrators based on their userPrincipalName",
            "IsAssignableToRole": false,
            "RestrictedAU": true,
            "RestrictedAUName": "Protected-Personas"
        },
        {
            "id": "P200",
            "Name": "P-Internals",
            "Type": "dynamic",
            "Rule": "(user.companyname -eq \"contoso\")",
            "Description": "Contains all employees based on the companyname attribute",
            "IsAssignableToRole": false,
            "RestrictedAU": false
        },
        {
            "id": "P300",
            "Name": "P-Guests",
            "Type": "dynamic",
            "Rule": "(user.userType -eq \"guest\")",
            "Description": "Containts all guest accounts based on the type of identity in Entra ID",
            "IsAssignableToRole": false,
            "RestrictedAU": false
        },
        {
            "id": "P400",
            "Name": "P-GenericAccount",
            "Type": "dynamic",
            "Rule": "(user.userPrincipalName -startswith \"g-\")",
            "Description": "Containts all generic accounts based on their userPrincipalName",
            "IsAssignableToRole": false,
            "RestrictedAU": false
        },
        {
            "id": "P500",
            "Name": "P-ServiceAccounts",
            "Type": "dynamic",
            "Rule": "(user.userPrincipalName -startswith \"svc-\")",
            "Description": "Containts all service accounts based on their userPrincipalName",
            "IsAssignableToRole": false,
            "RestrictedAU": false
        },
        {
            "id": "P600",
            "Name": "P-MTR",
            "Type": "manual",
            "Rule": "",
            "Description": "Containts all Microsoft Teams Rooms accounts",
            "IsAssignableToRole": false,
            "RestrictedAU": false
        },
        {
            "id": "P700",
            "Name": "P-PAWUsers",
            "Type": "dynamic",
            "Rule": "(user.userPrincipalName -startswith \"paw-\")",
            "Description": "Containts all PAW users based on their userPrincipalName",
            "IsAssignableToRole": false,
            "RestrictedAU": true,
            "RestrictedAUName": "Protected-Personas"
        }
